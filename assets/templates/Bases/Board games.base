filters:
  and:
    - categories.contains(link("Board games"))
    - '!file.name.contains("Template")'
properties:
  file.name:
    displayName: Game
  note.rating:
    displayName: Rating
  note.last:
    displayName: Last
views:
  - type: table
    name: Table
    order:
      - file.name
      - rating
      - last
